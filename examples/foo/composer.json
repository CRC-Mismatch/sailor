{
  "repositories": [
    {
      "type": "path",
      "url": "../../"
    }
  ],
  "require": {
    "spawnia/sailor": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3.4",
    "spawnia/phpunit-assert-directory": "^0.1.0"
  },
  "config": {
    "platform": {
      "php": "7.3.9"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Spawnia\\Sailor\\Foo": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spawnia\\Sailor\\Tests\\": "tests/",
      "Spawnia\\Sailor\\Foo\\": "examples/foo/expected/"
    }
  }
}
